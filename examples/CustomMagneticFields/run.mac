# minimal command set test
# verbose
/run/verbose 0
/tracking/verbose 0

# Set max step size
/MF/detector/setMaxStep 1

# run init
/run/initialize

# Magnetic Field
/field/setFieldZ 1

# Stepper choice
/field/setStepperType 1

# Set Trapping current (Amps)
/field/setCurrent 2

# Set Trap Radius (mm)
/field/setRadius 300

# start
/run/beamOn 1

